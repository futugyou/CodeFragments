      "GlobalHttpTimeOut": 5000,
      "MailOption": {
        "Server": "",
        "Port": 0,
        "User": "",
        "Password": "",
        "UseSsl": false,
        "AlertMailList": []
      },
      "DingTalkOption": {
        "Token": "",
        "AtPhones": "",
        "IsAtAll": false
      }
