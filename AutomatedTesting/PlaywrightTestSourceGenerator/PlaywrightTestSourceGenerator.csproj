<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>preview</LangVersion>
		<!-- <EnforceExtendedAnalyzerRules>true</EnforceExtendedAnalyzerRules> -->
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Json.Net" Version="1.0.33" PrivateAssets="all" />
		<PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.9.2" PrivateAssets="all" />
		<PackageReference Include="Microsoft.CodeAnalysis.Analyzers" Version="3.11.0-beta1.24318.1">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
		</PackageReference>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.10.0" GeneratePathProperty="true" PrivateAssets="all" />
		<PackageReference Include="Microsoft.Playwright.MSTest" Version="1.45.0" GeneratePathProperty="true" PrivateAssets="all" />
		<PackageReference Include="MSTest.TestAdapter" Version="3.5.0" GeneratePathProperty="true" PrivateAssets="all" />
		<PackageReference Include="MSTest.TestFramework" Version="3.5.0" GeneratePathProperty="true" PrivateAssets="all" />
	</ItemGroup>
